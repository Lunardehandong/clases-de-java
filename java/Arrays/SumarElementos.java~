public class SumarElementos {
    public static void main(String[] args) {
      //creamos el arreglo y le asignamos valores
        int[] numeros = {5, 10, 15, 20, 25};

        // Sumar los primeros tres elementos
        int suma = numeros[0] + numeros[1] + numeros[2] + numeros[3] + numeros[4];

        System.out.println("La suma de los primeros tres elementos es: " + suma);
    }
}
